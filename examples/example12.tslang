Deschodt factoriel(n -> int) -> int
    erif (n <= 1):
        deschodt 1
    deschelse:
        deschodt n * factoriel(n - 1)

Deschodt Eric() -> int
    eric val = 5 -> int
    eric fact = factoriel(val) -> int
    peric("factoriel({val}) = {fact}")
    deschodt 0

